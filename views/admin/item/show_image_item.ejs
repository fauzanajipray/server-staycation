<div
  class="tab-pane fade show active container-fluid"
  id="show-image"
  role="tabpanel"
  aria-labelledby="show-item-tab"
>
  <div class="row">
    <% for(let i = 0 ; i < item.imageId.length; i++) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="http://localhost:5000/<%= item.imageId[i].imageUrl%>"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
</div>
<script></script>
