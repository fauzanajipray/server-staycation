<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="2"
      >
        <thead>
          <tr>
            <th>No</th>
            <th>Bank Name</th>
            <th>Nomer Rekening</th>
            <th>Name</th>
            <th>Action</th>
          </tr>
        </thead>
        <tfoot>
          <tr>
            <th>No</th>
            <th>Bank Name</th>
            <th>Nomer Rekening</th>
            <th>Name</th>
            <th>Action</th>
          </tr>
        </tfoot>
        <tbody>
          <% for(let i=1; i< 12; i++) {%>
          <tr>
            <td><%= i %></td>
            <td>bank</td>
            <td>norek</td>
            <td>Nama</td>
            <td>View Edit Delete</td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>
